<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ro.bar.sanymotors.model.impl.AttributeImpl" table="attribute">
		<id name="elementId">
			<column name="elementid" />
			<generator class="increment" />
		</id>

		<!-- properties bellow -->
		<property name="name" column="name" />
		<property name="value" column="value" />
		<property name="interfaceValue" column="interfacevalue" type="java.lang.Boolean"/>
        <!-- dependencies -->
        
	</class>


</hibernate-mapping>